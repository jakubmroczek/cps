                        <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                        <NumberAxis side="LEFT" />
                    </yAxis>
                </BarChart>
            </children>
        </VBox>
        <VBox layoutX="696.0" prefHeight="188.0" prefWidth="200.0">
