                <HBox layoutX="10.0" layoutY="17.0" prefHeight="32.0" prefWidth="194.0">
                    <children>
                        <Label prefHeight="46.0" prefWidth="107.0" text="Moc sr.:" />
                        <Label fx:id="averagePowerValueLabel" prefHeight="32.0" prefWidth="87.0" />
